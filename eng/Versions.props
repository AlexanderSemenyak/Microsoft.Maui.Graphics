<Project>
  <!--Package versions-->
  <PropertyGroup>
    <!-- dotnet/installer -->
    <MicrosoftDotnetSdkInternalPackageVersion>6.0.100-rtm.21527.11</MicrosoftDotnetSdkInternalPackageVersion>
    <!-- dotnet/runtime -->
    <MicrosoftNETCoreAppRefPackageVersion>6.0.0</MicrosoftNETCoreAppRefPackageVersion>
    <!-- xamarin/xamarin-android -->
    <MicrosoftAndroidSdkWindowsPackageVersion>31.0.101-preview.11.74</MicrosoftAndroidSdkWindowsPackageVersion>
    <!-- xamarin/xamarin-macios -->
    <MicrosoftMacCatalystSdkPackageVersion>15.0.101-preview.10.175</MicrosoftMacCatalystSdkPackageVersion>
    <MicrosoftiOSSdkPackageVersion>15.0.101-preview.10.175</MicrosoftiOSSdkPackageVersion>
    <MicrosofttvOSSdkPackageVersion>15.0.101-preview.10.175</MicrosofttvOSSdkPackageVersion>
    <MicrosoftmacOSSdkPackageVersion>12.0.101-preview.10.175</MicrosoftmacOSSdkPackageVersion>
  </PropertyGroup>
  <PropertyGroup>
    <!-- Trim all characters after first `-` or `+` is encountered. -->
    <DotNetPreviewVersionBand Condition=" '$(DotNetPreviewVersionBand)' == '' ">$([System.Text.RegularExpressions.Regex]::Replace($(MicrosoftDotnetSdkInternalPackageVersion), `[-+].*$`, ""))</DotNetPreviewVersionBand>
  </PropertyGroup>
</Project>
